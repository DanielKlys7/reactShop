{
  "name": "react-interview-starter-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "coverage": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test",
    "serve": "./node_modules/.bin/http-server ./build --port 1337",
    "dockerized": "docker run -it --rm -v ${PWD}:/app -v /app/node_modules -p 3000:3000 -e CHOKIDAR_USEPOLLING=true app:dev"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^1.2.0",
    "@testing-library/jest-dom": "5.11.3",
    "@testing-library/react": "10.4.8",
    "@types/jest": "26.0.10",
    "@types/node": "14.6.0",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.1.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.13.1",
    "react-query": "^3.5.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "react-test-renderer": "16.13.1",
    "typescript": "3.9.7",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/jwt-decode": "^3.1.0",
    "@types/testing-library__jest-dom": "5.9.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "cross-env": "7.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "full-icu": "1.3.1",
    "http-server": "0.12.3",
    "prettier": "^2.2.1",
    "wait-on": "5.2.0"
  }
}
